<ejb-jar>
	<enterprise-beans>
		<entity>
		<ejb-name>BeanEJB</ejb-name>
		<home>BeanHome</home>
		<remote>Bean</remote>
		<ejb-class>Bean</ejb-class>
		<persistence-type>Bean</persistence-type>
