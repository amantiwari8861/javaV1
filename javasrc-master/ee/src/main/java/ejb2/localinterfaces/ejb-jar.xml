<!DOCTYPE ejb-jar PUBLIC 
	"-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
	"http://java.sun.com/dtd/ejb-jar_2_0.dtd"
>

<ejb-jar>
	<enterprise-beans>
		<entity>
		<ejb-name>MemberEJB</ejb-name>
		<local-home>ejb2.localinterfaces.MemberLocalHome</local-home>
		<local>ejb2.localinterfaces.MemberLocal</local>
		<ejb-class>ejb2.localinterfaces.MemberBean</ejb-class>
		<persistence-type>Container</persistence-type>
		<prim-key-class>java.lang.Integer</prim-key-class>
		<reentrant>false</reentrant>
		<cmp-version>2.x</cmp-version>
		<abstract-schema-name>MemberData</abstract-schema-name>
		</entity>
	</enterprise-beans>
</ejb-jar>
